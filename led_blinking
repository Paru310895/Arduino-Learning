// C++ code
//
int led1=7;

void setup()
{
  //pinMode(pin,mode); 
  //mode implies input pin or output pin
  pinMode(7, OUTPUT);
}

void loop()
{
  digitalWrite(led1, HIGH);//digitalWrite specifies where we have to give digital input 
  delay(1000); // Wait for 1000 millisecond(s)
  digitalWrite(led1, LOW);
  delay(1000); // Wait for 1000 millisecond(s)
}
